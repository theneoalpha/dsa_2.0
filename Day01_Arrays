
#include <iostream>
using namespace std;

int main() {
 int n;
 
    // reversing the integer in an array
    cout<<"Enter the size of an array:"<<endl;
    cin>>n;
    int arr[1000];
    cout<<"Enter the element of an array: ";
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    
    int i=0,j=n-1;
    while(j>i){
        swap(arr[i],arr[j]);
        i++;
        j--;
    }
    
    for(int i = 0;i<n;i++){
        cout<<arr[i];
    }

    return 0;
}